{
  "expo": {
    "name": "momentu",
    "slug": "momentu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": ".app/assets/logo-bw.png",
    "splash": {
      "image": ".app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f0f8ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": ".app/assets/adaptive-icon.png",
        "backgroundColor": "#f0f8ff"
      }
    },
    "web": {
      "favicon": ".app/assets/logo=bw.png"
    }
  }
}
